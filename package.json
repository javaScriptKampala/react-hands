  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "react": "^18.2.0",
    "rollup": "^3.20.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2"
